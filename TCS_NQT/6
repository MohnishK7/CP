//TCS NQT
   int n;
   cin >> n;
   vector<int> a(n);

   for (int i = 0; i < n; i++) {
      cin >> a[i];
   }

   sort(a.begin(), a.end());
   for (int i = 0; i < n / 2; i++) {
      cout << a[i] << " ";
   }
   for (int i = n - 1; i >= n / 2; i--) {
      cout << a[i] << " ";
   }

   return 0;
}
