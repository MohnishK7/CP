//TCS NQT
   int n;
   cin >> n;
   vector<int> a(n);

   for (int i = 0; i < n; i++) {
      cin >> a[i];
   }

   // reverse(a.begin(), a.end());
   int p1 = 0, p2 = n - 1;
   while (p1 < p2) {
      swap(a[p1], a[p2]);
      p1++; p2--;
   }
   for (int i = 0; i < n; i++) {
      cout << a[i] << " ";
   }
   return 0;
}
